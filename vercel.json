{
  "version": 2,
  "builds": [
    {
      "src": "crmproject/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "crmproject/wsgi.py"
    }
  ],
  "env": {
    "MYSQLCLIENT_CFLAGS": "/usr/include/mysql",
    "MYSQLCLIENT_LDFLAGS": "-L/usr/lib/x86_64-linux-gnu -lmysqlclient"
  },
  "installCommand": "pip install -r requirements.txt"
}
